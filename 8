package main
import "fmt"
func main() {
	//variables use colon with equal sign
	a :="hello world"
	b := "I will called Buddha"
	//PrintIn(s) will make a new line if you use more than 1
	//if you command PrintIn to show 2 variables with 1 command they will give space for you
	fmt.Print("hello world")
	fmt.Println(b)
	fmt.Print(a)
}